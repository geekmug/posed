<!DOCTYPE suppressions PUBLIC
"-//Puppy Crawl//DTD Suppressions 1.0//EN"
"http://www.puppycrawl.com/dtds/suppressions_1_1.dtd">
<suppressions>
  <!-- Suppress all checks for generated sources. -->
  <suppress files="[/\\]build[/\\]generated-sources[/\\]" checks=".*"/>

  <!-- Suppress low-value checks for test sources. -->
  <suppress files="[/\\]src[/\\]test[/\\]" checks="DesignForExtension" />
  <suppress files="[/\\]src[/\\]test[/\\]" checks="FinalClass" />
  <suppress files="[/\\]src[/\\]test[/\\]" checks="Javadoc*" />
  <suppress files="[/\\]src[/\\]test[/\\]" checks="MagicNumber" />
  <suppress files="[/\\]src[/\\]test[/\\]" checks="VisibilityModifier" />
</suppressions>
